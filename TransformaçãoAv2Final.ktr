// Função para limpar e padronizar o campo cidade
function limparCidade(cidade) {
    if (!cidade) return null; // Se o campo estiver vazio, retornar nulo

    // Remover espaços em branco extras, converter para minúsculas e capitalizar a primeira letra
    cidade = cidade.trim().toLowerCase().replace(/\b\w/g, function (char) {
        return char.toUpperCase();
    });

    // Mapear cidades conhecidas com abreviações para seus nomes completos
    var cidadesAbreviadas = {
        "Sp": "São Paulo",
        "Rj": "Rio de Janeiro",
        "Dc": "Duque de Caxias",
		"Rio": "Rio de Janeiro"
        // Adicione outras cidades conhecidas com abreviações conforme necessário
    };

    // Verificar se a cidade está na lista de abreviações conhecidas e substituir pelo nome completo
    if (cidadesAbreviadas[cidade]) {
        cidade = cidadesAbreviadas[cidade];
    }

    return cidade;
}

// Aplicar a função limparCidade ao campo 'cidade'
cidade = limparCidade(cidade);
</jsScript_script>
