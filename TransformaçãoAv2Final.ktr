function padronizarGenero(valor) {
    if (!valor) return "O"; // Se o valor for nulo, retornar "O" para outros

    valor = valor.toLowerCase().trim(); // Converter para minúsculas e remover espaços em branco

    if (valor === "feminino" || valor === "f" || valor === "mulher" || valor === "m") {
        return "F"; // Retornar "F" para feminino
    } else if (valor === "masculino" || valor === "m" || valor === "homem" || valor === "h") {
        return "M"; // Retornar "M" para masculino
    } else {
        return "O"; // Retornar "O" para outros casos
    }
}

